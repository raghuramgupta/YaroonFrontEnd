{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(favicon.ico|logo192.png|logo512.png|manifest.json|robots.txt)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
